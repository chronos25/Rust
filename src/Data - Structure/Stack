fn main() {
   let mut vec = Vec::new();
   //push operation
	vec.push(1);
	vec.push(2);   

assert_eq!(vec.len(), 2); //length of vector
assert_eq!(vec[0], 1);

assert_eq!(vec.pop(), Some(2));  //pop opertion in vector
assert_eq!(vec.len(), 1);

vec[0] = 0;
assert_eq!(vec[0], 0);// add element before 1st place

vec.extend([1, 2, 3].iter().cloned());

for x in &vec {
    println!("{}", x);
}
assert_eq!(vec, [0, 1, 2, 3]);
}
